FUNCTION main :
READ v1
v2 := v1
READ v3
v4 := v3
v19 := v1 + #1
v6 := v19
v21 := v1 + v3
v10 := v21
LABEL L1 :
IF #0 >= v2 GOTO L3
LABEL L2 :
v2 := v10
v22 := v10 - #1
v8 := v22
IF v6 >= v22 GOTO L5
LABEL L4 :
v6 := #4
v4 := v8
GOTO L6
LABEL L5 :
v23 := v6 + #3
v6 := v23
LABEL L6 :
GOTO L1
LABEL L3 :
WRITE v4
RETURN #0

